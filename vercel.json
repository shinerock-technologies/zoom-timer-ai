{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "VITE_OPENAI_API_KEY": "@vite-openai-api-key",
    "VITE_API_SECRET_TOKEN": "@vite-api-secret-token"
  }
}
